##  Rename me to .env and fill in the values

# FastAPI App configuration
APP_NANE=SmartChargeAPI
DEBUG=True
SECRET_KEY=mySecretKey

# Mysql Config
MYSQL_HOST=mysql
MYSQL_USER=root
MYSQL_PASSWORD=pwrd
MYSQL_DB=fastapi
